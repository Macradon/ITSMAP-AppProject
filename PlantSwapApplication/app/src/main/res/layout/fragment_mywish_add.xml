<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/wishPlantNameText"
        android:layout_width="222dp"
        android:layout_height="36dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="20dp" />

    <Spinner
        android:id="@+id/wishPlantNamepinner"
        android:layout_width="242dp"
        android:layout_height="34dp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/wishPlantRadiusText"
        android:layout_width="214dp"
        android:layout_height="33dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="124dp" />

    <Spinner
        android:id="@+id/wishPlantRadiusSpinner"
        android:layout_width="218dp"
        android:layout_height="35dp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/cancel_addwish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/back"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="655dp" />

    <Button
        android:id="@+id/save_addwish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/save"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="655dp" />
</androidx.constraintlayout.widget.ConstraintLayout>